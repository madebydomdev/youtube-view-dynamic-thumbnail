{
  "configurations": [
    {
      "type": "aws-sam",
      "request": "direct-invoke",
      "name": "youtube-view-dynamic-thumbnail:src/handlers/app.handler (nodejs20.x)",
      "invokeTarget": {
        "target": "code",
        "projectRoot": "${workspaceFolder}/",
        "lambdaHandler": "src/handlers/app.handler"
      },
      "lambda": {
        "runtime": "nodejs20.x",
        "payload": {},
        "environmentVariables": {
          "NODE_ATTRIBUTE": "@_id",
          "NODE_VALUE": "view-count"
        }
      }
    }
  ]
}
